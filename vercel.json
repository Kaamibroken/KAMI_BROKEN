{
  "version": 2,
  "builds": [
    {
      "src": "database/database.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "database/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/database",
      "dest": "database/database.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
